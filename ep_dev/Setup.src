proc,JtDebug=1

rout,Setup(§ext,§dir)
{
	var,§l
	§l=a

	if,"defprc(`epEnv`)=0"
	then
	{
		epEnv={util}
		pgu,epEnv
	}

	!prce,epDev%
	epDevDir={util}\ep_dev
	pgu,epDev%

	!prce,jtIcon[§ext]%
	!prce,jtMenu[§ext]%
	jtIcon[§ext]"¹Dev.¹"=
	jtMenu[§ext]"¹Dev.¹"=
	jtMenu[§ext][§l]"¹Config. proc. env.¹"=lanc,LancChEnv,{epDevDir}\epChEnv{ex}
	§l=[chr(asc(§l)+1)]

	mnu
}