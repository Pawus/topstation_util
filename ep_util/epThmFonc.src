rout,DeleteFpar
/*	OBSOLETE	*/
{
	/*	On parcourt la liste des parcelles	*/
	runs,Cpar
	{
		var,§com,§sec,§num,§suppr
		§suppr=0
		§com=[[mm]&V1:com]
		§sec=[[mm]&V1:sec]
		§num=[[mm]&V1:num]
		/*	On parcourt, pour chaque parcelle, la liste des assiettes de volumétries	*/
		runs,Fpar
		{
			/*	Si une parcelle est assiette de volumétrie, §suppr vaut 1, sinon il vaut 0	*/
			if,[[[mm]&V1:com]=§com]@[[[mm]&V1:sec]=§sec]@[[[mm]&V1:num]=§num]
			{
				§suppr=1
			}
			else
			{
				§suppr=0
			}
		}
		/*	Si §suppr vaut 1, on supprime le composant Cpar	*/
		if,§suppr=1
		{
			cmpf,[mm] /* pas bon */
		}
	}
}