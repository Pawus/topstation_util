/*	@	24/02/2012	*/
/*  @	Création de composants DZ à partir des géométries Cpar, avec même matricule que Cpar	*/
/*	@	Suppression des composants DZ correspondant à des parcelles qui font partie d'assiettes de volumétries	*/
/*	@	Nécessite les composants Cpar et Fpar chargés dans la base, et nécessite DZ non chargé ou vide	*/
/*	@	Auteur : Vianney Dugrain	*/

/*	Lancement : lanc,SelectToDZ,D:\travail\TopStation\Fichiers source\themFoncier.src	*/

/*	proc,epMdV=EP_ZON_THM_PROP	*/

rout,Main
{
	odbc,data=Cadastre,auto=1			
	lanc,ImpTout,[epUtil]\epImCdif.src	
	run,SelectToDZ
	run,DeleteFED
	run,TracePlans
}

rout,SelectToDZ
/*	Créée les composants DZ à partir des composants d'une selection	*/
{
	var,§cmp,§dz
	!Select=Cpar.3228-Cpar.3239
	runs,!Select
	{
		§cmp=[mm]
		§dz=[ml(`DZ`)]
		[§dz]&R0:F=[§cmp]
		[§dz]:MD=EP_ZON_THM_PROP
		[§dz]&Z0:[§cmp]
		[§dz]&q0:dtc="[[§dz]:JO]/[[§dz]:MO]/20[[§dz]:AN]",io="EPADESA-DFJ",in="calculé"
	}
}


rout,CparToDZ
/*	Créée les composants DZ à partir des géométries Cpar	*/
{
	var,§cmp
	
	runs,Cpar.3228-Cpar.3239
	/*	Pour chaque parcelle, création d'un nouveau composant DZ	*/
	{
		§cmp=[mm]
		DZ.[[§cmp]:i]:MD=EP_THM_PROP
		DZ.[[§cmp]:i]&Z0:[§cmp]
	}
	marq,10,`Cpar vers DZ`
}

rout,DeleteFED
/*	Supprime dans DZ les parcelles qui sont comprises dans les assiettes de volumétrie	*/
{
	runs,FED
	/*	Pour chaque volumétrie, suppression dans DZ des parcelles intersectées	*/
	{
		!cmpe,DZ*!zoni([mm],-0.50)
	}
	marq,10,`Suppression des assiettes de volumétrie`
}

